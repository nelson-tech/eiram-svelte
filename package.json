{
	"name": "eiram",
	"version": "0.0.1",
	"scripts": {
		"dev": "vite dev --port 8141",
		"build": "vite build",
		"package": "svelte-kit package",
		"preview": "vite preview",
		"check": "svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check --plugin-search-dir=. . && eslint .",
		"format": "prettier --write --plugin-search-dir=. .",
		"generate": "houdini generate",
		"pm2-dev": "pm2 start npm --name \"Eiram Svelte Dev\" -- run dev",
		"pm2-dev-stop": "pm2 stop \"Eiram Svelte Dev\" && pm2 delete \"Eiram Svelte Dev\"",
		"pm2-dev-restart": "pm2 restart \"Eiram Svelte Dev\"",
		"pm2-start": "pm2 start npm --name \"Eiram Svelte Production\" -- run start",
		"pm2-stop": "pm2 stop \"Eiram Svelte Production\" && pm2 delete \"Eiram Svelte Production\"",
		"pm2-restart": "pm2 restart \"Eiram Svelte Production\"",
		"pm2-node-start": "PORT=8141 pm2 start --name \"Eiram Node\" node dist/index.js",
		"pm2-node-stop": "pm2 stop --name \"Eiram Node\" && pm2 delete \"Eiram Node\"",
		"pm2-node-restart": "pm2 restart \"Eiram Node\""
	},
	"devDependencies": {
		"@felte/reporter-svelte": "^1.1.4",
		"@felte/validator-zod": "^1.0.12",
		"@rgossiaux/svelte-headlessui": "^1.0.2",
		"@rollup/plugin-typescript": "9.0.1",
		"@steeze-ui/heroicons": "^2.1.0",
		"@steeze-ui/svelte-icon": "^1.3.1",
		"@sveltejs/adapter-auto": "^1.0.0-next.83",
		"@sveltejs/adapter-node": "^1.0.0-next.98",
		"@sveltejs/kit": "^1.0.0-next.516",
		"@tailwindcss/typography": "^0.5.4",
		"@types/cookie": "^0.5.1",
		"@typescript-eslint/eslint-plugin": "^5.40.0",
		"@typescript-eslint/parser": "^5.40.0",
		"autoprefixer": "^10.4.8",
		"cookie": "^0.5.0",
		"eslint": "^8.25.0",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"felte": "^1.2.6",
		"houdini": "^0.16.8",
		"jwt-decode": "^3.1.2",
		"postcss": "^8.4.18",
		"prettier": "^2.6.2",
		"prettier-plugin-svelte": "^2.8.0",
		"sass": "^1.54.9",
		"stripe": "^10.14.0",
		"svelte": "^3.51.0",
		"svelte-check": "^2.9.2",
		"svelte-preprocess": "^4.10.7",
		"svelte-stripe": "^0.0.19",
		"sveltekit-autoimport": "^1.6.9",
		"tailwindcss": "^3.1.8",
		"tslib": "^2.3.1",
		"typescript": "^4.8.4",
		"unfetch": "^4.2.0",
		"vite": "^3.1.8",
		"zod": "^3.18.0"
	},
	"type": "module",
	"dependencies": {
		"@xstate/svelte": "^2.0.0",
		"xstate": "^4.33.0"
	}
}
